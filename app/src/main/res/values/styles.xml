    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@android:color/holo_red_dark</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/material_grey_600</item>
        <item name="android:colorBackground">@color/material_grey_900</item>
        <item name="android:textColorPrimary">@color/abc_primary_text_disable_only_material_dark</item>
